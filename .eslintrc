{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-param-reassign": 0,
        "prefer-promise-reject-errors": 0,
        "no-shadow": 0,
        "consistent-return": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": 0,
        "no-console": 0,
        "linebreak-style": 0,
        "no-param-reassign": 0,
        "no-multi-spaces": 0,
        "operator-linebreak": 0,
        "func-names": 0,
        "prettier/prettier": [
            "error",
            {
              "trailingComma": "all",
              "singleQuote": true,
              "printWidth": 80,
              "semi": true,
              "endOfLine": "auto"
            }
          ]
    }
}
